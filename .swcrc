{
	"$schema": "https://json.schemastore.org/swcrc",
	"sourceMaps": true,
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"decorators": true,
			"dynamicImport": true
		},
		"baseUrl": "./",
		"target": "es2021",
		"keepClassNames": true,
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"minify": true
}
